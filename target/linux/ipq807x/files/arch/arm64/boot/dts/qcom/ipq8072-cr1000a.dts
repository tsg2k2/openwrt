/dts-v1/;

#include "ipq8074.dtsi"
#include "ipq8074-hk-cpu.dtsi"
#include "ipq8074-ess.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Verizon CR1000A";
	compatible = "verizon,cr1000a", "qcom,ipq8074";

	aliases {
		ethernet4 = &dp5;
		ethernet5 = &dp6;
		sdhc1 = &sdhc_1;
		serial0 = &blsp1_uart5;
		label-mac-device = &dp5;

	};
	
	chosen {
		stdout-path = "serial0:115200n8";
	};

	keys {
		compatible = "gpio-keys";
		pinctrl-0 = <0x5b>;
		pinctrl-names = "default";

		reset {
			debounce-interval = <0x3c>;
			gpios = <0x1f 0x2c 0x01>;
			label = "reset";
			linux,code = <0x198>;
			linux,input-type = <0x01>;
		};

		wps {
			debounce-interval = <0x3c>;
			gpios = <0x1f 0x2a 0x01>;
			label = "wps";
			linux,code = <0x211>;
			linux,input-type = <0x01>;
		};
	};
};

&blsp1_uart5 {
	status = "okay";
};

&prng {
	status = "okay";
};

&cryptobam {
	status = "okay";
};

&crypto {
	status = "okay";
};

&edma {
	status = "okay";
};

&qpic_bam {
	status = "okay";
};

&sdhc_1 {
	status = "okay";
};

&mdio {
	status = "okay";

	rtl9301: ethernet-phy@0 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <0x0>;
	};

	aqr113c: ethernet-phy@8 {
		compatible = "ethernet-phy-ieee802.3-c45";
		reg = <0x08>;
	};
};

&switch {
	status = "okay";

	pinctrl-names = "default";
	switch_lan_bmp = <0x3e>;
	switch_wan_bmp = <0x40>;
	switch_mac_mode = <0x00>;
	switch_mac_mode1 = <0x0e>;
	switch_mac_mode2 = <0x0d>;
	bm_tick_mode = <0x00>;
	tm_tick_mode = <0x00>;

	qcom,port_phyinfo {
		
		port@4 {
			port_id = <5>;
			ethernet-phy-ieee802.3-c45;
			phy_address = <0x0>;
		};

		port@5 {
			port_id = <6>;
			ethernet-phy-ieee802.3-c45;
			phy_address = <0x8>;
		};
	};

};

&ssphy_0 {
	status = "okay";
};

&ssphy_1 {
	status = "okay";
};

&qusb_phy_0 {
	status = "okay";
};

&qusb_phy_1 {
	status = "okay";
};

&usb_0 {
	status = "okay";

	// vbus-supply = <&reg_usb_vbus>;
};

&usb_1 {
	status = "okay";

	// vbus-supply = <&reg_usb_vbus>;
};

&wifi {
	status = "okay";
	qcom,ath11k-calibration-variant = "Verizon-CR1000A";
};

&dp5_syn {
	status = "okay";
	label = "lan";
	phy-handle = <&rtl9301>;
};

&dp6_syn {
	status = "okay";
	label = "wan";
	phy-handle = <&aqr113c>;
};

&pcie0 {
	status = "okay";
};

&pcie_qmp0 {
	status = "okay";
};

&pcie1 {
	status = "okay";
};

&pcie_qmp1 {
	status = "okay";
};
